                                                            $item("#s3b").hide();
                                                            $item("#s4b").hide();
                                                            $item("#s5b").hide();

                                                            //$item("#b1b").hide();
                                                            //$item("#b2b").hide();
                                                            //$item("#b3b").show();
                                                            //$item("#b4b").show();
